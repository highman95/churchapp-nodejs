{
  "name": "churchapp-nodejs",
  "version": "1.0.0",
  "description": "churchapp built on nodejs",
  "main": "src/index.js",
  "scripts": {
    "start": "node --trace-warnings ./bin/www",
    "dev": "nodemon ./bin/www -e js,hbs",
    "test": "jest",
    "test-o": "jest --maxWorkers=1 --forceExit",
    "test-w": "jest --watchAll --detectOpenHandles --maxWorkers=1 --forceExit"
  },
  "jest": {
    "collectCoverage": true,
    "verbose": true,
    "testPathIgnorePatterns": [
      "bin/www",
      "__tests__/utils.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/highman95/churchapp-nodejs.git"
  },
  "keywords": [
    "churchapp",
    "church-records",
    "church-attendance"
  ],
  "author": "Emma Nwamaife",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/highman95/churchapp-nodejs/issues"
  },
  "homepage": "https://github.com/highman95/churchapp-nodejs#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-ensure-login": "^0.1.1",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.7",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "express-mysql-session": "^3.0.0",
    "express-session": "^1.17.1",
    "graphql": "^15.8.0",
    "hbs": "^4.1.2",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "redis": "^4.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "nodemon": "^2.0.7"
  }
}
